<!DOCTYPE html>
<html>
<head><title>Admin Dashboard</title></head>
<body>
  <h2>All Registered Users</h2>
  <table border="1">
    <tr>
      <th>Username</th><th>Email</th><th>Phone</th><th>Ticket Type</th><th>Registered</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.ticketType %></td>
        <td><%= user.registeredAt.toDateString() %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
